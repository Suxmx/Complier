docker run -it --rm -v $(pwd):/root/compiler maxxing/compiler-dev bash

build/compiler -koopa hello.c -o hello.koopa

build/compiler -dump hello.c -o hello.dump

build/compiler -riscv hello.c -o hello.riscv

mkdir build && cd ./build  && rm compiler && wget https://github.com/Suxmx/Complier/releases/download/build/compiler && cd..

git push -f origin main:buildBranch